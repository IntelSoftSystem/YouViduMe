{
  "name": "y-v-m",
  "version": "1.0.0",
  "description": "Electron application for YouViduMe",
  "main": "./build/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IntelSoftSystem/YouViduMe.git"
  },
  "productName": "YouViduMe",
  "scripts": {
    "start": "webpack --config ./webpack.main.js --mode development && concurrently 'npm:watch' 'electron ./build/main.js'",
    "clean": "rm -rf node_modules build dist",
    "install": "electron-rebuild",
    "lint": "eslint . && flow",
    "build": "webpack --config ./webpack.main.js --mode production && webpack --config ./webpack.renderer.js --mode production",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "validate": "npm ls",
    "watch": "webpack --config ./webpack.renderer.js --mode development --watch --watch-poll",
    "mac": "electron-builder -m",
    "win": "electron-builder -w",
    "make-cert": "electron-builder create-self-signed-cert -p IntelSoft",
    "publish": "GH_TOKEN='f777f066a8c6fd40f4f72303ca2cf974382b0e6c' electron-builder build -m  --publish always onTagOrDraft"
  },
  "build": {
    "appId": "com.electron.y-v-m",
    "productName": "YouViduMe",
    "generateUpdatesFilesForAllChannels": true,
    "publish": [
      {
        "provider": "github",
        "owner": "IntelSoftSystem",
        "repo": "YouViduMe"
      }
    ],
    "files": [
      "**/*",
      "resources",
      "!app",
      "!main.js"
    ],
    "mac": {
      "artifactName": "YouViduMe.${ext}",
      "category": "public.app-category.video"
    },
    "linux": {
      "artifactName": "YouViduMe-${arch}.${ext}",
      "category": "VideoConference;AudioVideo;Audio;Video;Network",
      "description": "YouViduMe Desktop App",
      "executableName": "YouViduMe",
      "target": [
        {
          "arch": "x64",
          "target": "AppImage"
        }
      ],
      "publish": [
        "github"
      ]
    },
    "win": {
      "artifactName": "YouViduMe.${ext}",
      "certificateFile": "private/IntelSoft.pfx",
      "verifyUpdateCodeSignature": false,
      "publisherName": "IntelSoft",
      "target": [
        {
          "arch": [
            "ia32",
            "x64"
          ],
          "target": "nsis"
        }
      ],
      "publish": [
        "github"
      ]
    },
    "directories": {
      "buildResources": "resources"
    }
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "jingle",
    "webrtc",
    "xmpp",
    "electron",
    "jitsi-meet"
  ],
  "author": "Intelsoft Systems <support@jitsi.org>",
  "readmeFilename": "README.md",
  "license": "Apache-2.0",
  "dependencies": {
    "@atlaskit/avatar": "14.1.1",
    "@atlaskit/button": "9.0.13",
    "@atlaskit/css-reset": "3.0.2",
    "@atlaskit/droplist": "7.0.10",
    "@atlaskit/field-text": "7.0.11",
    "@atlaskit/icon": "14.0.1",
    "@atlaskit/navigation": "33.2.0",
    "@atlaskit/onboarding": "6.0.2",
    "@atlaskit/page": "8.0.7",
    "@atlaskit/spinner": "9.0.9",
    "@atlaskit/theme": "6.1.0",
    "@atlaskit/toggle": "5.0.9",
    "babel": "^6.23.0",
    "electron-debug": "2.0.0",
    "electron-is-dev": "1.0.1",
    "electron-log": "^2.2.17",
    "electron-reload": "1.2.5",
    "electron-window-state": "5.0.1",
    "fs": "0.0.1-security",
    "fs-exists-sync": "^0.1.0",
    "history": "4.7.2",
    "javascript": "^1.0.0",
    "jitsi-meet-electron-utils": "github:jitsi/jitsi-meet-electron-utils#1972c3bf0884ace68eb496894dabae593d6dbf49",
    "js-utils": "github:jitsi/js-utils#0c53500a5120be2aa3fc590f0f932a0d4771920f",
    "moment": "2.22.2",
    "mousetrap": "1.6.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-modal": "^3.8.1",
    "react-redux": "5.0.7",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "^4.0.1",
    "redux-logger": "3.0.6",
    "redux-persist": "5.10.0",
    "redux-persist-electron-storage": "1.1.2",
    "source-map-support": "0.5.9",
    "styled-components": "3.4.10",
    "update-electron-app": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-transform-flow-strip-types": "7.0.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "concurrently": "4.0.1",
    "css-loader": "1.0.0",
    "devtron": "1.4.0",
    "electron": "3.0.3",
    "electron-builder": "^20.40.2",
    "electron-react-devtools": "0.5.3",
    "electron-rebuild": "^1.8.4",
    "electron-updater": "^4.0.6",
    "eslint": "^5.16.0",
    "eslint-config-jitsi": "github:jitsi/eslint-config-jitsi#v0.1.0",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsdoc": "3.8.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "2.0.0",
    "flow-bin": "0.81.0",
    "html-webpack-plugin": "3.2.0",
    "precommit-hook": "3.0.0",
    "style-loader": "0.23.1",
    "svg-inline-loader": "0.8.0",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2"
  }
}
